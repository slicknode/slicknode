{
  "name": "@slicknode/core",
  "version": "0.16.0",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"nodemon build/rootApi.js\"",
    "build": "npm run clean && npm run compile && npm run copy-files",
    "build:test": "npm run clean && npm run compile:test && npm run copy-files:test",
    "compile": "tsc -p tsconfig.build.json",
    "compile:test": "tsc -p tsconfig.test.json",
    "watch": "tsc -p tsconfig.build.json --watch",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "npm run build:test && npm run test:unit && npm run test:integration",
    "watch:test": "npm run copy-files:test && tsc -p tsconfig.test.json --watch",
    "testonly": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha -n experimental-modules -n es-module-specifier-resolution=node --exit --full-trace --timeout 10000 --check-leaks -r esm 'build/**/__tests__/**/*-test.js'",
    "test:integration": "mocha -n experimental-modules -n es-module-specifier-resolution=node --exit --full-trace --timeout 10000 --check-leaks -r esm 'build/**/__integration__/**/*-test.js'",
    "clean": "rimraf build/*",
    "prepublishOnly": "npm run clean && npm run build",
    "copy-files": "copyfiles -e '**/__integration__/**' -e '**/__tests__/**' -u 1 'src/**/*.{ico,yaml,yml,json,txt,html,graphql}' build",
    "copy-files:test": "copyfiles -u 1 'src/**/*.{ico,yaml,yml,json,txt,html,graphql,png,jpg,svg,txt,gif,tiff,bmp,webp}' build",
    "prepare": "npm run build"
  },
  "directories": {
    "lib": "./build"
  },
  "repository": "slicknode/slicknode",
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@slicknode/client-node": "workspace:*",
    "@slicknode/runtime-executor": "workspace:*",
    "adm-zip": "^0.4.14",
