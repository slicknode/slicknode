{
  "name": "@slicknode/project-admin-api",
  "version": "0.2.1",
  "main": "dist/index.js",
  "repository": "slicknode/slicknode",
  "author": "Slicknode LLC",
  "license": "OSL-3.0",
  "scripts": {
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:test": "tsc -p tsconfig.test.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "eslint ./src --ext .ts",
    "watch:test": "tsc -w -p tsconfig.test.json",
    "clean": "rimraf dist/*",
    "test": "npm run lint && npm run build:test && npm run testonly",
    "testonly": "mocha -n experimental-modules -n es-module-specifier-resolution=node --exit --full-trace --timeout 10000 --check-leaks -r esm 'dist/**/{__integration__,__tests__}/**/*-test.js'",
    "prepublishOnly": "npm run clean && npm run build",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "npm run build"
  },
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@slicknode/core": "workspace:*",
    "@types/adm-zip": "^0.5.0",
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.45",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "adm-zip": "^0.5.10",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "esm": "^3.2.25",
    "express": "^4.18.0",
    "graphql": "^15.8.0",
    "graphql-query-test-mock": "^0.12.1",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@slicknode/core": "*",
    "express": "^4.17.1",
    "graphql": "*"
  },
  "dependencies": {
    "@slicknode/client-node": "workspace:*",
    "@slicknode/project-config-schema": "workspace:*",
    "@slicknode/project-config-transform": "workspace:*",
    "graphql-tag": "^2.12.6",
    "joi": "^17.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
