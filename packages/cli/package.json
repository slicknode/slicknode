{
  "name": "slicknode",
  "version": "0.13.0",
  "description": "GraphQL-First Application Framework and CMS for rapid software development: Serverless, declarative and extensible.",
  "bin": {
    "slicknode": "./bin/run"
  },
  "bugs": "https://github.com/slicknode/slicknode/issues",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "npm run clean && tsc -b && oclif-dev manifest && oclif-dev readme && npm run copyfiles",
    "test": "npm run testonly",
    "test:coverage": "nyc --extension .ts mocha --forbid-only 'test/**/*.test.ts' 'src/**/__tests__/**/*-test.{ts,tsx}'",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "format": "prettier --config .prettierrc '(src|test)/**/*.ts' --write",
    "build": "tsc",
    "clean": "rimraf lib/",
    "watch": "tsc --watch",
    "copyfiles": "copyfiles -u 2 \"./src/templates/**/*\" ./lib/templates",
    "testonly": "mocha --exit --full-trace --require ts-node/register \"src/**/__tests__/**/*-test.{ts,tsx}\" \"test/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build && npm run copyfiles"
  },
  "directories": {
    "lib": "./dist"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "adm-zip": "^0.5.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "indent-string": "^4.0.0",
    "inquirer": "^7.3.3",
    "joi": "^17.3.0",
    "js-yaml": "^3.14.1",
    "listr": "^0.14.3",
    "listr-verbose-renderer": "^0.6.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "slicknode-client": "^0.4.1",
    "slicknode-runtime": "^0.3.0",
    "tar": "^6.0.5",
    "tslib": "^1",
    "uuid": "^8.3.2",
    "validator": "^13.5.2",
    "wrap-ansi": "^7.0.0"
  },
  "peerDependencies": {},
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/README.md",
    "/LICENSE"
  ],
  "repository": "slicknode/slicknode",
  "keywords": [
    "graphql",
    "slicknode",
    "headless",
    "serverless",
    "framework",
    "cms"
  ],
  "author": "Ivo Mei√üner",
  "license": "MIT",
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/adm-zip": "^0.4.31",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.3",
    "@types/copyfiles": "^2.1.1",
    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/inquirer": "^0.0.43",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/js-yaml": "^3.11.2",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^10.0.3",
    "@types/node-fetch": "^2.1.2",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^10.0.2",
    "@types/strip-ansi": "^5.2.1",
    "@types/supertest": "^2.0.8",
    "@types/tar": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^9.4.1",
    "@types/wrap-ansi": "^3.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^2.4.1",
    "mocha": "^5.2.0",
    "mocha-sinon": "^2.1.0",
    "nock": "^13.0.5",
    "nyc": "^14.1.1",
    "prettier": "^2.3.1",
    "sinon": "^11.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.8.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "slicknode",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
